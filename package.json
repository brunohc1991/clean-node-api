    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "rimraf": "^4.4.0",
    "sucrase": "^3.29.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
